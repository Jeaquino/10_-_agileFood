        <div class="contenedor">
            <form class="mtx">
                <div>
                    <label class="col-3" for="">Producto</label>
                    <input type="text">
                </div>
                <br>
                <div>
                    <label class="col-3" for="">Precio</label>
                    <input type="number">
                </div>
                <br>
                <div>
                    <label class="col-3 col-md-3" for="">Categoria</label>
